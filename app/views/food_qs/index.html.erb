<h1>Listing food_qs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_qs.each do |food_q| %>
      <tr>
        <td><%= link_to 'Show', food_q %></td>
        <td><%= link_to 'Edit', edit_food_q_path(food_q) %></td>
        <td><%= link_to 'Destroy', food_q, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food q', new_food_q_path %>
